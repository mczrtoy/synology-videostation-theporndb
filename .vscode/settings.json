// Place your settings in this file to overwrite default and user settings.
{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git/": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/tmp/*/**": true,
        "**/vendor/**": true
    },
    "files.trimTrailingWhitespace": true,
    "php.validate.enable": true,
}